{
  "name": "cvml-platform-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@mui/icons-material": "5.8.4",
    "@okta/okta-auth-js": "6.9.0",
    "@okta/okta-react": "6.7.0",
    "@reduxjs/toolkit": "1.9.2",
    "@vitejs/plugin-react": "4.2.1",
    "aws-cdk-lib": "2.115.0",
    "constructs": "10.0.5",
    "eslint": "8.56.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "2.0.4",
    "react-redux": "8.0.5",
    "react-router-dom": "6.22.3",
    "typescript": "5.4.3",
    "vite": "5.1.7",
    "vite-tsconfig-paths": "4.3.1",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "test": "cross-env TZ=America/Chicago vitest",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint --ext js,jsx,ts,tsx --max-warnings=0 src aws-cdk",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "npx stylelint \"src/**/*.css\"",
    "stylelint:fix": "npm run stylelint \"src/**/*.css\" -- --fix",
    "verify": "cross-env TZ=America/Chicago npm run lint && npm run stylelint && npm run api:coverage -- --run && npm run cdk:test -- --run && npm run test:coverage -- --run",
    "cdk": "cdk",
    "cdk:test": "vitest --config aws-cdk/vitest.config.js",
    "api:test": "vitest --config lambdas/vitest.config.js",
    "api:coverage": "npm run api:test -- --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rtk-query/codegen-openapi": "1.2.0",
    "@svgr/webpack": "6.3.1",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-flatpickr": "3.8.7",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "@vitest/coverage-v8": "1.5.2",
    "aws-cdk": "2.115.0",
    "aws-sdk": "2.1528.0",
    "cross-env": "7.0.3",
    "cypress": "12.14.0",
    "esbuild": "0.19.11",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-n": "15.2.4",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "happy-dom": "13.3.8",
    "stylelint-config-css-modules": "4.1.0",
    "stylelint-config-standard": "26.0.0",
    "ts-node": "10.9.2",
    "vitest": "1.5.2",
    "vitest-fetch-mock": "0.2.2"
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "0.20.2",
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  },
  "overrides": {
    "@svgr/webpack": "$@svgr/webpack"
  },
  "engines": {
    "node": "20"
  },
  "resolutions": {
    "nth-check": "2.1.1"
  },
  "proxy": "http://127.0.0.1:3001"
}